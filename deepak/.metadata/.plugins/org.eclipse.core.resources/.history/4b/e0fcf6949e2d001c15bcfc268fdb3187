package pattern;

public class Pluspattern {

	public static void main(String[] args) {
		int i=10 , j=10;
		
		for(i=0;i<=4;i++) {
			for(j=0;j<=4;j++) {
				if(j==0||i==4) {
					System.out.print("*");
			}else if(i==0) {
				System.out.println("*");
			}
				
				else if(j==4) {
					System.out.println("* ");
				}
				else {
					System.out.print(" ");
				}
				
			}
			System.out.println();
		}
	}

}
