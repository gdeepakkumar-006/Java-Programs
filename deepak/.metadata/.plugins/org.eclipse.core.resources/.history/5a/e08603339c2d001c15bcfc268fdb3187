package pattern;

public class Pluspattern {

	public static void main(String[] args) {
		int i=6 , j=6;
		
		for(i=0;i<5;i++) {
			for(j=0;j<=4;j++) {
				if(j==5||i==2) {
					System.out.print("* ");
				}else {
					System.out.print(" ");
				}
				
			}
			System.out.println();
		}
	}

}
